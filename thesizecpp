#include<iostream>
using namespace std;
int main (){
float d=34.4F;
long double e = 34.4L;
cout<<"The size of 34.4 is "<<sizeof(34.4);
cout<<"The size of 34.4f is "<<sizeof(34.4f);
cout<<"The size of 34.4F is "<<sizeof(34.4F);
cout<<"The size of 34.4l is "<<sizeof(34.4l);
cout<<"The size of 34.4L is "<<sizeof(34.4L);
cout <<"The value of d is "<<d <<"The value of e is "<<e;
return 0;
}
